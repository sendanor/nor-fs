    "nor-function" : ">= 0.1.5 < 0.2.0",
    "nor-array" : ">= 0.1.5 < 0.2.0",
    "nor-extend" : ">= 0.1.27 < 0.2.0",
    "nor-is"     : ">= 0.1.16 < 0.2.0"
  },
  "devDependencies": {
    "lint": "*",
    "mocha": "*",
    "jshint" : "*"
